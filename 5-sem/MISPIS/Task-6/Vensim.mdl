Меркутова Кристина 3 группа=
	6
	~	
	~		|

error at the end of Scrum sprint or in Waterfall=IF THEN ELSE ( switch=0 , 0.4 , 0.05\
		 )
	~	
	~		|

"waterfall - 0 scrum - 1 kanban - 2"=0
	~	
	~		|

work to do in Kanban=PULSE(0, 4) + PULSE TRAIN(1, 1, 5, 2000) * IF THEN ELSE ( Original work to do\
		>0 , 1 , 0 )
	~	
	~		|

requirement developments rate=IF THEN ELSE ( switch=2 , IF THEN ELSE ( Selected Requirements\
		 > productivity * n0 developers
	 , (productivity * n0 developers) * (1 - error in Kanban) , Selected Requirements * \
		(1 - error in Kanban) ) , IF THEN ELSE
	 ( Selected Requirements > productivity*n0 developers , (productivity * n0 developers
	) * (1 - error in sprint Scrum or in Waterfall) , Selected Requirements ) )
	~	
	~		|

input requirements=IF THEN ELSE ( switch=0 , Product Backlog , IF THEN ELSE ( switch=\
		1 , 0 , IF THEN ELSE ( Product Backlog>=n0 requirements to do in each iteration , Product Backlog\
		 , 0 ) )  )
	~	
	~		|

rework discovery in Scrum or Waterfall=IF THEN ELSE ( switch=2 , 0 , IF THEN ELSE(Selected Requirements\
		 < 0.05,Fraction work done * error at the end of Scrum sprint or in Waterfall
	,0) )
	~	
	~		|

error in Kanban=0.1
	~	
	~		|

n0 requirements to do in each iteration=IF THEN ELSE ( switch=1 , rework discovery in Scrum or Waterfall\
		 , IF THEN ELSE ( switch=2 , 5 , 0 )  )
	~	
	~		|

input features=Product Backlog
	~	
	~		|

Selected Requirements= INTEG (IF THEN ELSE ( switch=0 , rework discovery+input requirements\
		-requirement developments rate , IF THEN ELSE ( switch=1 , rework discovery-requirement developments rate\
		+selected features in output , -requirement developments rate+input requirements ) \
		),
		0)
	~	
	~		|

delay=IF THEN ELSE ( switch=0 , 120 , 0 )
	~	
	~		|

delay in scrum sprint=5
	~	
	~		|

selected features in output=IF THEN ELSE ( Sprint Backlog>0 , PULSE TRAIN(0, 1, delay in scrum sprint\
		, 2000)* IF THEN ELSE ( Selected Requirements<=0 , 10 , 0 ) , 0 )
	~	
	~		|

Original work to do= INTEG (IF THEN ELSE ( switch=2 , -work to do in Kanban , -work in input\
		 ) ,
		210)
	~	
	~		|

Sprint Backlog= INTEG (input features-selected features in output,
		0)
	~	
	~		|

switch=0
	~	
	~		|

rework discovery=IF THEN ELSE ( switch=0 , Rework , IF THEN ELSE ( switch=1 , PULSE TRAIN\
		 ( 0 , 1 , delay in scrum sprint , 2000 ) * IF THEN ELSE 
	( Selected Requirements<=0 , Rework , 0 ) , 0 )  )
	~	
	~		|

Product Backlog= INTEG (IF THEN ELSE ( switch=0 , work in output-input requirements ,\
		 IF THEN ELSE ( switch=1 , work in output-input features , work to do in Kanban-input requirements\
		 ) ),
		0)
	~	
	~		|

work done rate=IF THEN ELSE ( switch=0 , IF THEN ELSE (Selected Requirements<0.05, Fraction work done\
